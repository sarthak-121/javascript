{

    "manifest_version": 2,
    "name": "DDictionary",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content_script.js"]
        }
      ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "./image/icon.png",
        "default_popup": "./popup/popup.html",
        "default_tile": "DDictionary"
    }

}